  "tokenColors": [
    // Comments
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#008000",
        "fontStyle": "italic"
      }
    },
    // Strings
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#A31515"
      }
    },
    // Numbers
    {
      "name": "Number",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.numeric.hex"
      ],
      "settings": {
        "foreground": "#098658"
      }
    },
    // Constants
    {
      "name": "Constant",
      "scope": [
        "constant",
        "constant.language",
        "constant.character"
      ],
      "settings": {
        "foreground": "#0451A5"
      }
    },
    // Keywords
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#AF00DB"
      }
    },
    // Storage (function, class, var, let, const, etc)
    {
      "name": "Storage",
      "scope": [
        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#0000FF"
      }
    },
    // Function names
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#795E26"
      }
    },
    // Classes & Types
    {
      "name": "Class & Type",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#267F99"
      }
    },
    // Variables
    {
      "name": "Variable",
      "scope": [
        "variable",
        "variable.other"
      ],
      "settings": {
        "foreground": "#001080"
      }
    },
    // Variable - Parameter
    {
      "name": "Parameter",
      "scope": [
        "variable.parameter",
        "meta.parameter"
      ],
      "settings": {
        "foreground": "#001080"
      }
    },
    // Property
    {
      "name": "Property",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "support.variable.property",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#001080"
      }
    },
    // Operators
    {
      "name": "Operator",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.comparison"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    // Punctuation
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    // Tags (HTML/JSX)
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#800000"
      }
    },
    // Attributes
    {
      "name": "Attribute",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#E50000"
      }
    },
    // CSS Selectors
    {
      "name": "CSS Selector",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#800000"
      }
    },
    // CSS Property Names
    {
      "name": "CSS Property",
      "scope": [
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#E50000"
      }
    },
    // CSS Values
    {
      "name": "CSS Value",
      "scope": [
        "support.constant.property-value.css"
      ],
      "settings": {
        "foreground": "#0451A5"
      }
    },
    // Regular Expressions
    {
      "name": "RegExp",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#811F3F"
      }
    },
    // Escape Characters
    {
      "name": "Escape Character",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#EE0000"
      }
    },
    // Markdown
    {
      "name": "Markdown Heading",
      "scope": [
        "markup.heading",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#0078D4",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
